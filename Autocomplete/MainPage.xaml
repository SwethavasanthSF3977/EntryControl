             xmlns:local="clr-namespace:Autocomplete"
             x:Class="Autocomplete.MainPage">

    <HorizontalStackLayout  Spacing="30" HorizontalOptions="Center" >


     <VerticalStackLayout >
         <HorizontalStackLayout Margin="0,100,0,0">
             <Label Text="Entry "  Margin="0,10,10,0">
             </Label>
                <Entry WidthRequest="200">                    
                </Entry>
         </HorizontalStackLayout>
     </VerticalStackLayout>
 </HorizontalStackLayout>
</ContentPage>
